{
  "name": "igara.github.io",
  "engines": {
    "node": ">=18.7.0",
    "npm": "use pnpm",
    "pnpm": ">=7",
    "yarn": "use pnpm"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "json:api": "(cd packages/json && pnpm run json:api)",
    "json:thema": "(cd packages/json && pnpm run json:thema)",
    "json:build": "(cd packages/json && pnpm run build)",
    "ui:types": "(cd packages/ui && pnpm run build:types)",
    "ui:build": "(cd packages/ui && pnpm run build)",
    "web:start": "(cd packages/web && pnpm run dev)",
    "web:export": "(cd packages/web && pnpm run export)",
    "lint": "pnpm run lint:eslint && pnpm run lint:stylelint",
    "lint:ui": "(cd packages/ui && pnpm run lint)",
    "lint:web": "(cd packages/web && pnpm run lint)",
    "lint:stylelint": "stylelint \"**/src/**/*.css\""
  },
  "dependencies": {
    "@babel/core": "7.18.10",
    "@emotion/babel-preset-css-prop": "11.10.0",
    "@igara.github.io/stylelint-plugin-thema-rule": "./packages/stylelint-plugin-thema-rule",
    "@storybook/addon-actions": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/addon-interactions": "6.5.10",
    "@storybook/addon-links": "6.5.10",
    "@storybook/builder-webpack5": "6.5.10",
    "@storybook/jest": "0.0.10",
    "@storybook/manager-webpack5": "6.5.10",
    "@storybook/react": "6.5.10",
    "@storybook/testing-library": "0.0.13",
    "@types/react": "18.0.15",
    "babel-loader": "8.2.5",
    "classnames": "2.3.1",
    "jest": "28.1.3",
    "jest-specific-snapshot": "5.0.0",
    "prettier": "2.7.1",
    "raw-loader": "4.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "require-from-string": "2.0.2",
    "storybook-css-modules": "1.0.8",
    "stylelint": "14.11.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-standard": "28.0.0",
    "stylelint-prettier": "2.0.0",
    "typescript": "4.7.4",
    "webpack": "5.74.0"
  }
}
